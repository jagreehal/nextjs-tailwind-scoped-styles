{
  "name": "nextjs-tailwind-scoped-styles",
  "version": "1.0.0",
  "scripts": {
    "dev": "next ./src -p ${PORT-5002}",
    "build": "next build ./src",
    "export": "rimraf node_modules/.cache && rimraf src/out && npm run build && next export ./src -o out",
    "deploy": "npm run export && touch out/.nojekyll && gh-pages -d out -t true",
    "start": "next start ./src -p ${PORT-5002}"
  },
  "dependencies": {
    "next": "^10.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^10.2.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  }
}
